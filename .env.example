# Copy this file to .env and fill in your actual values

# Get these from Google Cloud Console OAuth 2.0 Client
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Generate a random secret for cookie signing (use a tool like `openssl rand -hex 32`)
COOKIE_SECRET=

# OAuth redirect URI - update the domain/port as needed
REDIRECT_URI=http://localhost:8501/oauth2callback

# Google's OpenID Connect configuration URL (this should not change)
SERVER_METADATA_URL=https://accounts.google.com/.well-known/openid-configuration

# Groq API Key https://console.groq.com/keys
GROQ_API_KEY=

# Google Generative Language API Key https://aistudio.google.com/api-keys
GOOGLE_API_KEY=